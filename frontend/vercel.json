{
  "redirects": [
    {
      "source": "/",
      "destination": "/movies",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://app-movie-backend.vercel.app/api/:path*"
    },

    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
